{
  "name": "seznam-driver",
  "version": "0.0.3",
  "description": "Custom Seznam driver for Adonis Ally",
  "main": "build/providers/index.js",
  "types": "build/providers/index.d.ts",
  "files": [
    "build/src",
    "build/providers",
    "build/standalone.js",
    "build/standalone.d.ts",
    "build/instructions.md"
  ],
  "scripts": {
    "clean": "del build",
    "copyfiles": "copyfiles \"instructions.md\" build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile && npm run copyfiles",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "version": "npm run build"
  },
  "keywords": [
    "social-auth",
    "adonisjs",
    "adonis-ally"
  ],
  "author": "Knihobot s.r.o.",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/ally": "^5.0.0-7",
    "@adonisjs/core": "^6.1.5-32",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@swc/core": "^1.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@adonisjs/ally": "^5.0.0-7",
    "@adonisjs/core": "^6.1.5-32"
  },
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "env": {
      "SEZNAM_CLIENT_ID": "",
      "SEZNAM_CLIENT_SECRET": ""
    },
    "types": "seznam-driver",
    "providers": [
      "seznam-driver"
    ]
  }
}